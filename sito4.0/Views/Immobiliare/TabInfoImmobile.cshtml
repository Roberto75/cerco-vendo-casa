@model Annunci.Models.Immobile



<dl class="row">
    <dt class="col-sm-3 col-6">Data inserimento</dt>
    <dd class="col-sm-3 col-6">@Model.dataInserimento.ToShortDateString()</dd>
    <dt class="col-sm-3 col-6">Tipo</dt>
    <dd class="col-sm-3 col-6">@Html.DisplayFor(model => model.immobile)</dd>

    <dt class="col-sm-3 col-6">Mq</dt>
    <dd class="col-sm-3 col-6">@Model.MQ.ToString("N")</dd>
    <dt class="col-sm-3 col-6">Prezzo</dt>
    <dd class="col-sm-3 col-6">@Model.prezzo.ToString("C")</dd>




    <dt class="col-sm-3 col-6">Utente</dt>
    <dd class="col-sm-3 col-6">@Html.DisplayFor(model => model.login)</dd>
    <dt class="col-sm-3 col-6">Categoria</dt>
    <dd class="col-sm-3 col-6">@Html.DisplayFor(model => model.categoria)</dd>
    <dt class="col-sm-3 col-6">Classe energetica</dt>
    <dd class="col-sm-3 col-6">@MyHelper.decodeEnum(Model.classeEnergetica.ToString())</dd>



    <dt class="col-sm-3 col-6">Stato</dt>
    <dd class="col-sm-3 col-6">@MyHelper.decodeEnum(Model.statoImmobile.ToString())</dd>
    <dt class="col-sm-3 col-6">Piano</dt>
    <dd class="col-sm-3 col-6">@MyHelper.decodeNull(Model.piano)</dd>
    <dt class="col-sm-3 col-6">Ascensore</dt>
    <dd class="col-sm-3 col-6">@MyHelper.decodeSiNo(Model.ascensore)</dd>
    <dt class="col-sm-3 col-6">Portiere</dt>
    <dd class="col-sm-3 col-6">@MyHelper.decodeSiNo(Model.portiere)</dd>



    <dt class="col-sm-3 col-6">Occupazione</dt>
    <dd class="col-sm-3 col-6">@MyHelper.decodeEnum(Model.occupazione.ToString())</dd>
    <dt class="col-sm-3 col-6">Totale piani edificio</dt>
    <dd class="col-sm-3 col-6">@MyHelper.decodeNull(Model.pianiTotali)</dd>
    <dt class="col-sm-3 col-6">Anno costruzione</dt>
    <dd class="col-sm-3 col-6">@MyHelper.decodeNull(Model.anno)</dd>
    <dt class="col-sm-3 col-6">Spese condominiali</dt>
    <dd class="col-sm-3 col-6">@MyHelper.decodeNull(Model.speseCondominiali)</dd>


    <!--  -->

    <dt class="col-sm-3 col-6">Cucina</dt>
    <dd class="col-sm-3 col-6">@MyHelper.decodeEnum(Model.cucina.ToString())</dd>
    <dt class="col-sm-3 col-6">Camere da letto</dt>
    <dd class="col-sm-3 col-6">@MyHelper.decodeNumeroStanze(Model.camereDaLetto)</dd>
    <dt class="col-sm-3 col-6">Bagni</dt>
    <dd class="col-sm-3 col-6">@MyHelper.decodeNumeroStanze(Model.bagni)</dd>
    <dt class="col-sm-3 col-6">Terrazzi</dt>
    <dd class="col-sm-3 col-6">@MyHelper.decodeNumeroStanze(Model.terrazzi)</dd>
    <dt class="col-sm-3 col-6">Cantina</dt>
    <dd class="col-sm-3 col-6">@MyHelper.decodeSiNo(Model.cantina)</dd>
    <dt class="col-sm-3 col-6">Posto auto</dt>
    <dd class="col-sm-3 col-6">@MyHelper.decodeEnum(Model.postoAuto.ToString())</dd>
    <dt class="col-sm-3 col-6">Riscaldamento</dt>
    <dd class="col-sm-3 col-6">@MyHelper.decodeEnum(Model.riscaldamento.ToString())</dd>

    <dt class="col-sm-3 col-6">Salone</dt>
    <dd class="col-sm-3 col-6">@MyHelper.decodeEnum(Model.salone.ToString())</dd>
    <dt class="col-sm-3 col-6">Camerette</dt>
    <dd class="col-sm-3 col-6">@MyHelper.decodeNumeroStanze(Model.camerette)</dd>
    <dt class="col-sm-3 col-6">Giardino mq</dt>
    <dd class="col-sm-3 col-6">@MyHelper.decodeNull(Model.giardinoMq)</dd>
    <dt class="col-sm-3 col-6">Balconi</dt>
    <dd class="col-sm-3 col-6">@MyHelper.decodeNumeroStanze(Model.balconi)</dd>
    <dt class="col-sm-3 col-6">Soffitta</dt>
    <dd class="col-sm-3 col-6">@MyHelper.decodeSiNo(Model.soffitta)</dd>
    <dt class="col-sm-3 col-6">Box</dt>
    <dd class="col-sm-3 col-6">@MyHelper.decodeEnum(Model.box.ToString())</dd>
    <dt class="col-sm-3 col-6">Climatizzato</dt>
    <dd class="col-sm-3 col-6">@MyHelper.decodeSiNo(Model.climatizzato))</dd>

</dl>



    <hr />

  <dl class="row">


      <dt class="col-sm-3 col-6">Regione - Provincia</dt>
      <dd class="col-sm-3 col-6">@MyHelper.getLocalitaImmobileOrizzontale(Model)</dd>
      <dt class="col-sm-3 col-6">Zona/Quartiere</dt>
      <dd class="col-sm-3 col-6">@MyHelper.decodeNull(Model.quartiere)</dd>


      <dt class="col-sm-3 col-6">CAP</dt>
      <dd class="col-sm-3 col-6">@MyHelper.decodeNull(Model.cap)</dd>
      <dt class="col-sm-3 col-6">Indirizzo</dt>
      <dd class="col-sm-3 col-6">@MyHelper.decodeNull(Model.indirizzo)</dd>

 </dl>